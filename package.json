    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babel-register": "^6.4.3",
    "bdd-stdin": "^0.2.0",
    "chai": "^3.5.0",
    "codecov.io": "^0.1.6",
    "commitizen": "^2.5.0",
    "cz-conventional-changelog": "^1.1.5",
    "eslint": "^1.10.3",
    "ghooks": "^1.0.3",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.2",
    "gulp-blanket-mocha": "0.0.4",
